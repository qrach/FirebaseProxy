{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/prox/**",
        "function": "prox"
      }
    ],
    "remoteconfig": {
      "template": "remoteconfig.template.json"
    }
  }
}
